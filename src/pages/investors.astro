   const modal = document.getElementById("investorModal");
   const confirmCheck = document.getElementById("confirmCheck");
   const confirmCheck2 = document.getElementById("confirmCheck2");
   const confirmBtn = document.getElementById("confirmBtn");
   const cancelBtn = document.getElementById("cancelBtn");
 
   // Show modal on page load
   window.onload = () => {
     modal.style.display = "flex";
     confirmBtn.disabled = true; // Ensure it's disabled initially
   };
 
   // Enable Confirm only when BOTH checkboxes are checked
   function updateConfirmButtonState() {
     if (confirmCheck.checked && confirmCheck2.checked) {
       confirmBtn.disabled = false;
     } else {
       confirmBtn.disabled = true;
     }
   }
 
   confirmCheck.addEventListener("change", updateConfirmButtonState);
   confirmCheck2.addEventListener("change", updateConfirmButtonState);
 
   // Confirm button click
   confirmBtn.onclick = () => {
     modal.style.display = "none";
     alert("You have accepted the disclaimer.");
